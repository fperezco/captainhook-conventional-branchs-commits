{
    "name": "fperezco/captainhook-conventional-branchs-commits",
    "description": "Captain hook conventional branchs and commits is just a 'hook' to check that a commit, in a conventional branching model, follows the conventional rules.",
    "type": "captain-hook",
    "require": {
        "php": ">=7.4",
        "captainhook/captainhook": "^5.18"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "fperezco\\CaptainhookConventionalBranchCommits\\": "src/"
        }
    },
    "authors": [
        {
            "name": "fperezco"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "bin": [
        "bin/captainhook-conventional-branchs-commits"
    ]
}
